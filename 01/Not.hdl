/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=in,b=in,out=out);
}

// 0 = (1) => Not(1 and 1) => Not(1)
// 1 = (0) => Not(0 and 0) => Not(0)
